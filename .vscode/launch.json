{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python Debugger: FastAPI -- LOCAL",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app.main:app",
          "--host", "0.0.0.0",
          "--port", "8000",
          "--reload",
          "--log-level", "debug"
        ],
        "jinja": true,
        "env": {
          "PYTHONUNBUFFERED": "1"
        }
      }
    ]
  }
  